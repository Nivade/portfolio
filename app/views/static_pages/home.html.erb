<section id="about">
  <div id="map" class="container-fluid map"> </div>
  <%= javascript_include_tag 'maps'%>
  <script src="https://maps.googleapis.com/maps/api/js?key=<%= @google_api_key %>&callback=initMap" async defer></script>
</section>

<!-- Scroll to Top Button (Only visible on small and extra-small screen sizes) -->
<div class="scroll-top page-scroll hidden-sm hidden-xs hidden-lg hidden-md">
    <a class="btn btn-primary" href="#page-top">
        <%= fa_icon 'chevron-up' %>
    </a>
</div>

<!-- Portfolio Modals -->
<div class="portfolio-modal modal fade" id="portfolioModal1" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-content">
        <div class="close-modal" data-dismiss="modal">
            <div class="lr">
                <div class="rl">
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="modal-body">
                        <h2>Project Title</h2>
                        <hr class="star-primary">
                        <%= image_tag 'cabin.png', :class => 'img-responsive img-centered' %>
                        <p>Bacon ipsum dolor amet kevin boudin corned beef chicken, tail alcatra ground round. Jowl shank chicken, spare ribs leberkas beef ribeye kielbasa sirloin frankfurter rump venison brisket corned beef pig.</p>
                        <ul class="list-inline item-details">
                            <li>Client:
                                <strong>Willy
                                </strong>
                            </li>
                            <li>Date:
                                <strong>April 2016
                                </strong>
                            </li>
                            <li>Service:
                                <strong>Koekjes Bakken
                                </strong>
                            </li>
                        </ul>
                        <button type="button" class="btn btn-default" data-dismiss="modal"><%= fa_icon 'times' %> Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
